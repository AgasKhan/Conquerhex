.titleText {
    color: rgb(173, 144, 91);
    justify-content: flex-start;
    align-self: center;
    align-items: auto;
    font-size: 40px;
    left: 54px;
    padding-left: 0;
    margin-left: 0;
}

.verticalContainer {
    justify-content: space-around;
    align-self: center;
}

.horizontalContainer {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
    align-self: center;
}

.subTitle {
    color: rgb(255, 255, 255);
    font-size: 20px;
}

.margin20 {
    margin-top: 20px;
    margin-right: 20px;
    margin-bottom: 20px;
    margin-left: 20px;
}

.padding20 {
    padding-top: 20px;
    padding-right: 20px;
    padding-bottom: 20px;
    padding-left: 20px;
}

.verticalList {
    justify-content: flex-start;
    align-self: flex-start;
    flex-wrap: nowrap;
}

.horizontalList {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-self: center;
    flex-shrink: 0;
}

.listButton {
    background-color: rgb(41, 39, 39);
    border-left-color: rgb(130, 98, 41);
    border-right-color: rgb(130, 98, 41);
    border-top-color: rgb(130, 98, 41);
    border-bottom-color: rgb(130, 98, 41);
}

.slotContainerStyle {
    flex-grow: 0;
    width: 110px;
    flex-shrink: 0;
    height: 120px;
}

.slotTextStyle {
    color: rgb(178, 145, 86);
    flex-shrink: 1;
    width: auto;
    height: auto;
    -unity-text-align: middle-center;
    white-space: normal;
    text-overflow: ellipsis;
    overflow: hidden;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 15px;
    padding-right: 0;
    flex-grow: 1;
    -unity-font-style: normal;
    -unity-text-outline-width: 0.5px;
    -unity-text-outline-color: rgb(135, 110, 65);
    text-shadow: 0 0 43.2px rgb(91, 72, 36);
}

.listItemText {
    width: 25%;
    justify-content: flex-start;
    align-self: center;
    color: rgb(255, 255, 255);
    text-overflow: ellipsis;
    overflow: hidden;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 4px;
    padding-right: 4px;
    padding-bottom: 4px;
    padding-left: 4px;
    font-size: 12px;
    -unity-text-align: upper-center;
}

.ListItemButton {
    flex-grow: 0;
    flex-shrink: 0;
    height: 40px;
    width: 100%;
    background-color: rgba(25, 29, 35, 0.86);
    transition-duration: 0.15s;
    transition-timing-function: ease;
}

.ListItemButton:hover {
    background-color: rgb(57, 65, 75);
}

.closeButton {
    position: absolute;
    transform-origin: right bottom;
    top: 1012px;
    left: 1765px;
    width: 120px;
    background-color: rgb(21, 20, 20);
    color: rgb(255, 255, 255);
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    font-size: 20px;
    height: 45px;
}

.slotButtonClicked {
    background-color: rgb(48, 47, 47);
    border-left-color: rgb(255, 237, 206);
    border-right-color: rgb(255, 237, 206);
    border-top-color: rgb(255, 237, 206);
    border-bottom-color: rgb(255, 237, 206);
    scale: 0.9 0.9;
}

.slotTextClicked {
    color: rgb(255, 237, 206);
}

.opacityVisible {
    opacity: 1;
}

.opacityHidden {
    opacity: 0;
}

.changeButton {
    visibility: visible;
    border-left-color: rgb(217, 188, 103);
    border-right-color: rgb(217, 188, 103);
    border-top-color: rgb(217, 188, 103);
    border-bottom-color: rgb(217, 188, 103);
    background-color: rgb(49, 47, 41);
    color: rgb(255, 225, 138);
    width: 100px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    align-self: center;
    font-size: 12px;
}

.changeButton:hover {
    border-left-color: rgb(248, 207, 83);
    border-right-color: rgb(248, 207, 83);
    border-top-color: rgb(248, 207, 83);
    border-bottom-color: rgb(248, 207, 83);
    background-color: rgb(60, 58, 51);
    color: rgb(248, 207, 83);
    scale: 1.05 1.05;
}

